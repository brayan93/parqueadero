extends ../layout

block content
    .container
        .row
            .col-12
                table.table.table-bordered
                    tr
                        th Placa
                        th Hora
                    if registros
                        each reg in registros
                            tr
                                td #{reg.placa}
                                td #{reg.horaEntrada}
                    else
                        tr
                            td No se encuentran registros